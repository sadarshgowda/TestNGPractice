<suite name = "ParameterizedTestSuite">
    <parameter name="a" value = "5" />
    <parameter name="b" value = "5" /> <!-- parameters defined inside suite level has low prio -->
    <test name = "ParameterTests">
        <parameter name="a" value = "1" />
        <parameter name="b" value = "2" />
        <parameter name="a" value = "2" />
        <parameter name="b" value = "3" />  <!-- it will always takes the last set even if we are passing mutiple set in a singe test methd -->
        <classes>
            <parameter name="a" value = "10" />
            <parameter name="b" value = "10" />
            <class name = "parameters.MyTestClass"/>
        </classes>
    </test>

    <test name = "ParameterTest">
        <parameter name="a" value = "1" />
        <parameter name="b" value = "2" />
        <classes>
            <class name = "parameters.MyTestClass"/>
        </classes>
    </test>
</suite>